# Supabase Configuration
SUPABASE_URL=https://YOUR-PROJECT.supabase.co
SUPABASE_SERVICE_KEY=replace_me
SUPABASE_STORAGE_BUCKET=viral-videos

# Processing Configuration
MAX_CONCURRENT_DOWNLOADS=3
DOWNLOAD_TIMEOUT_SEC=180
MAX_VIDEO_SIZE_MB=500
TEMP_DIR=./temp
KEEP_TEMP_FILES_DAYS=7

# YT-DLP Configuration
YTDLP_FORMAT=best[ext=mp4]/best
YTDLP_COOKIES_BROWSER=chrome
YTDLP_RETRIES=3
YTDLP_FRAGMENT_RETRIES=3

# Storage Organization
STORAGE_PATH_PATTERN={year}/{month}/{username}_{post_id}_{timestamp}.mp4

# Retry Configuration
MAX_RETRY_ATTEMPTS=3
RETRY_BACKOFF_BASE=60
